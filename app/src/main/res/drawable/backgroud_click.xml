<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Set màu nền-->
    <solid
        android:color="#EFEEE9" >
    </solid>

    <!-- đặt thuộc tính màu và độ rộng của đường viền -->
